<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.a.SignUp">
   <children>
      <AnchorPane layoutX="428.0" prefHeight="613.0" prefWidth="404.0" style="-fx-background-color: darkorchid;">
         <children>
            <Label layoutX="51.0" layoutY="175.0" prefHeight="86.0" prefWidth="321.0" text="Welcome!" textFill="#fffefe">
               <font>
                  <Font name="Arial Black" size="58.0" />
               </font>
            </Label>
            <Label layoutX="27.0" layoutY="280.0" prefHeight="28.0" prefWidth="350.0" text="To keep connected with us please login" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label layoutX="107.0" layoutY="310.0" prefHeight="26.0" prefWidth="189.0" text="with your personal info" textFill="WHITE">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="55.0" layoutY="109.0" prefHeight="62.0" prefWidth="307.0" text="Create Account">
         <font>
            <Font name="Arial Black" size="34.0" />
         </font>
      </Label>
      <TextField fx:id="U" layoutX="54.0" layoutY="218.0" prefHeight="34.0" prefWidth="290.0" promptText="Username" style="-fx-background-color: #ffffff; -fx-border-color: #9370db; -fx-border-width: 0px 0px 2px 0px;" />
      <PasswordField fx:id="P" layoutX="54.0" layoutY="293.0" prefHeight="28.0" prefWidth="290.0" promptText="Password" style="-fx-background-color: #ffffff; -fx-border-width: 0px 0px 2px 0px; -fx-border-color: #9370db;" />
      <Button fx:id="login" layoutX="244.0" layoutY="487.0" mnemonicParsing="false" onAction="#ssignUp" prefHeight="46.0" prefWidth="134.0" style="-fx-background-radius: 15px; -fx-background-color: #9370db; -fx-text-fill: white;" text="Sign Up" textFill="MEDIUMPURPLE">
         <font>
            <Font name="Gadugi" size="20.0" />
         </font>
      </Button>
      <RadioButton fx:id="male" layoutX="54.0" layoutY="352.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="103.0" text="Male" textFill="DARKORCHID">
         <font>
            <Font size="14.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="d" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="female" layoutX="157.0" layoutY="352.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="103.0" text="Female" textFill="DARKORCHID" toggleGroup="$d">
         <font>
            <Font size="14.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="other" layoutX="270.0" layoutY="352.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="83.0" text="Other" textFill="DARKORCHID" toggleGroup="$d">
         <font>
            <Font size="14.0" />
         </font>
      </RadioButton>
      <TextField fx:id="E" layoutX="55.0" layoutY="405.0" prefHeight="34.0" prefWidth="288.0" promptText="Email" style="-fx-background-color: white; -fx-border-color: #9370db; -fx-border-width: 0px 0px 2px 0px;" />
      <Button fx:id="login1" layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onMouseClicked="#llogin" prefHeight="26.0" prefWidth="103.0" style="-fx-background-radius: 15px; -fx-background-color: #9370db; -fx-text-fill: white;" text="Login" textFill="MEDIUMPURPLE">
         <font>
            <Font name="Gadugi" size="20.0" />
         </font>
      </Button>
      <Label fx:id="lb1" layoutX="55.0" layoutY="252.0" prefHeight="18.0" prefWidth="103.0" text="UserName Empty" textFill="RED" />
      <Label fx:id="lb2" layoutX="55.0" layoutY="321.0" prefHeight="18.0" prefWidth="103.0" text="Password Empty" textFill="RED" />
      <Label fx:id="lb3" layoutX="54.0" layoutY="439.0" prefHeight="18.0" prefWidth="83.0" text="Email Empty" textFill="RED" />
      <Label fx:id="lb4" layoutX="55.0" layoutY="387.0" text="Empty" textFill="RED" />
   </children>
</AnchorPane>
